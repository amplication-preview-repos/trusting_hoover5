datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Locations {
  city      String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  state     String?
  updatedAt DateTime @updatedAt
  zipCode   String?
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  phone     String?
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}
